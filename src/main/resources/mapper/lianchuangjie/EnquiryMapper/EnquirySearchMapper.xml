<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lianchuangjie.lianchuangjie.mapper.EnquiryMapper.EnquirySearchMapper">

    <select id="selectList" resultType="com.lianchuangjie.lianchuangjie.vo.Enquiry.EnquirySearchResultVO">
        SELECT
            T_OBTN.ItemName AS 'Modle'
        FROM (
                 SELECT
                     WhsCode,
                     MdAbsEntry,
                     SUM(Quantity) AS Quantity,
                     SUM()
                 FROM T_OBTQ GROUP BY MdAbsEntry, WhsCode
        ) T
        INNER JOIN T_OBTN ON T.MdAbsEntry = T_OBTN.AbsEntry

    </select>
</mapper>