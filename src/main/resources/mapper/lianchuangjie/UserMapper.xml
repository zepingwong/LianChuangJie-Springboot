<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lianchuangjie.lianchuangjie.mapper.UserMapper">
    <select id="selectByUserCode" resultType="com.lianchuangjie.lianchuangjie.entity.UserEntity">
        SELECT TOP (1) *
        FROM T_OUSI
        WHERE UserCode = #{UserCode}
    </select>
</mapper>
